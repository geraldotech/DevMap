const container = document.getElementById("main");

class Box extends React.Component {
  printNome() {
    return <h1>func this.React printNome </h1>;
  }
  render() {
    //const e fun nativa
    const name = "Geraldo";
    function printx(n) {
      return n;
    }
    return (
      <div>
        <h1>Ola da class box</h1>
        <h2>{name}</h2>
        <p>{printx("printx Function")}</p>
        <p>{this.printNome()}</p>
      </div>
    );
  }
}

class Formulario extends React.Component {
  render() {
    return (
      <form>
        <input type="text" />
      </form>
    );
  }
}

ReactDOM.render(
  <div>
    <Formulario />
    <Box />
  </div>,
  container
);
